is.$store.state.DirsFileList=res.data.data\n            for(let i=0;i<this.$store.state.DirsFileList.length;i++){\n            this.$store.state.DirsFileList[i]={'index':i,'data':this.$store.state.DirsFileList[i],\"FileType\":false}\n        }\n        })\n        axios({\n                url:this.$store.state.api+\"/OpenDir\",\n                method:'post',\n            }).then(res=>{\n                let h=Object.values(res.data.su)\n                for(let i=0;i<this.$store.state.DirsFileList.length;i++){\n                    for(let j=0;j<=h.length;j++){\n                        if(this.$store.state.DirsFileList[i].data==h[j]){\n                            this.$store.state.DirsFileList[i].FileType='文件夹'\n                            break\n                        }\n                    }\n                }\n                    \n            })\n       \n    },\n    methods:{\n    }\n}\n</script>\n<style scoped>\n.data{\n    display: flex\n}\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,YAAW,MAAO,+BAA8B;AACvD,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,OAAOC,OAAM,MAAO,0BAAyB;AAC7C,OAAOC,UAAS,MAAO,6BAA4B;AAEnD,eAAc;EACVC,IAAI,EAAC,cAAc;EACnBC,UAAU,EAAC;IACPL,YAAY;IAACC,SAAS;IAACC,OAAO;IAACC;EACnC,CAAC;EACDG,OAAOA,CAAA,EAAE;IACLP,KAAK,CAAC;MACFQ,GAAG,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,GAAG,GAAC,GAAG,GAAC,cAAc;MAC5CC,MAAM,EAAC,MAAM;MACbC,MAAM,EAAC;QACH,cAAc,EAAC;MACnB;IACJ,CAAC,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;MACT,IAAI,CAACN,MAAM,CAACC,KAAK,CAACM,YAAY,GAACD,GAAG,CAACE,IAAI,CAACA,IAAG;MAC3C,KAAI,IAAIC,CAAC,GAAC,CAAC,EAACA,CAAC,GAAC,IAAI,CAACT,MAAM,CAACC,KAAK,CAACM,YAAY,CAACG,MAAM,EAACD,CAAC,EAAE,EAAC;QACxD,IAAI,CAACT,MAAM,CAACC,KAAK,CAACM,YAAY,CAACE,CAAC,CAAC,GAAC;UAAC,OAAO,EAACA,CAAC;UAAC,MAAM,EAAC,IAAI,CAACT,MAAM,CAACC,KAAK,CAACM,YAAY,CAACE,CAAC,CAAC;UAAC,UAAU,EAAC;QAAK;MAC1G;IACA,CAAC;IACDlB,KAAK,CAAC;MACEQ,GAAG,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,GAAG,GAAC,UAAU;MACpCC,MAAM,EAAC;IACX,CAAC,CAAC,CAACE,IAAI,CAACC,GAAG,IAAE;MACT,IAAIK,CAAC,GAACC,MAAM,CAACC,MAA