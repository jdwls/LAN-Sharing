                \r\n               })\r\n               return 0\r\n            }\r\n            if (this.Uses.match(UsesCheck)==null){\r\n                ElMessageBox.alert(\"用户名只能包含大小写字母、数字和下划线，且长度在6到12个字符之间\", '用户名错误', {\r\n                    showConfirmButton:true,\r\n                    confirmButtonText: '确认',\r\n               }).catch(()=>{\r\n                    this.Uses=''\r\n                    this.Password=''\r\n                    \r\n               })\r\n               return 0\r\n            }\r\n            if (this.Password.match(passwordCheck)==null){\r\n                ElMessageBox.alert(\"一个大写字母、一个小写字母、一个数字和一个特殊字符，且长度至少为8个字符\", '密码错误', {\r\n                    showConfirmButton:true,\r\n                    confirmButtonText: '确认',\r\n               }).catch(()=>{\r\n                   this.Uses=''\r\n                   this.Password=''\r\n                    \r\n               })\r\n               console.log(this.Password );\r\n               \r\n               return 0\r\n            }\r\n            if (this.Password.match(passwordCheck)!=null && this.Uses.match(UsesCheck)!=null && this.Uses!='' && this.Password!='' ){\r\n                let MD5AESUses= CryptoJS.AES.encrypt(this.Uses, timestamp).toString();\r\n                let MD5AESPassword= CryptoJS.AES.encrypt(this.Password,timestamp).toString();\r\n                let MD5AESBefore=MD5AESUses+MD5AESPassword\r\n                let MD5AESBehind=CryptoJS.AES.encrypt(MD5AESBefore,timestamp).toString();\r\n                console.log(CryptoJS.AES.decrypt(MD5AESBehind, timestamp).toString())\r\n                console.log(CryptoJS.AES.decrypt(CryptoJS.AES.decrypt(MD5AESBehind, timestamp).toString(), timestamp))\r\n                console.log(MD5AESBehind);\r\n                console.log(timestamp);\r\n                \r\n                axios({ \r\n                    url:this.$store.state.api+'/login',\r\n                    method:\"get\",\r\n                    params:{\r\n                    'MD5AESBehind': MD5AESBehind,\r\n                    'Time':timestamp,\r\n                }\r\n                })\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n/* .Uses{\r\n   \r\n} */\r\nspan{\r\n    padding-right: 1%; \r\n      \r\n} \r\n.InputStyle{\r\n    padding-bottom: 2%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-content: center;\r\n}\r\n</style>"],"mappings":";;;EAEaA,KAAK,EAAC;AAAY;gEACnBC,mBAAA,CAAiB,cAAX,MAAI;;EAETD,KAAK,EAAC;AAAY;gEACnBC,mBAAA,CAAmB,cAAb,QAAM;;EAEXD,KAAK,EAAC;AAAY;;;;uBAP3BE,mBAAA,CAUM,cATFD,mBAAA,CAEM,OAFNE,UAEM,GADFC,UAAiB,EAAAC,YAAA,CAAmEC,mBAAA;IAHhGC,UAAA,EAGgDC,KAAA,CAAAC,IAAI;IAHpD,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAGgDH,KAAA,CAAAC,IAAI,GAAAE,MAAA;IAAEC,KAAkB,EAAlB;MAAA;IAAA,CAAkB;IAACC,WAAW,EAAC;6CAE7EZ,mBAAA,CAEM,OAFNa,UAEM,GADFC,UAAmB,EAAAV,YAAA,CAAmGC,mBAAA;IANlIC,UAAA,EAMkDC,KAAA,CAAAQ,QAAQ;IAN1D,uBAAAN,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAMkDH,KAAA,CAAAQ,QAAQ,GAAAL,MAAA;IAAEC,KAAkB,EAAlB;MAAA;IAAA,CAAkB;IAACC,WAAW,EAAC,OAAO;IAACI,IAAI,EAAC,UAAU;IAAC,eAAa,EAAb;6CAE3GhB,mBAAA,CAEM,OAFNiB,UAEM,GADFb,YAAA,CAAwCc,oBAAA;IAA5BC,OAAK,EAAAV,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAACU,QAAA,CAAAC,KAAK;;IATnCC,OAAA,EAAAC,QAAA,CASsC,MAAE,CATxCC,gBAAA,CASsC,IAAE,E;IATxCC,CAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}